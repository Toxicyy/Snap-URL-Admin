{
  "name": "snapurl-admin",
  "version": "1.0.0",
  "private": true,
  "description": "Professional URL shortening service admin panel with advanced analytics",
  "keywords": [
    "url-shortener",
    "admin-panel",
    "analytics",
    "next.js",
    "typescript",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out dist coverage reports",
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__/unit --passWithNoTests",
    "test:accessibility": "jest --testPathPattern=accessibility --passWithNoTests",
    "test:all": "npm run test:unit && npm run test:accessibility",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests --verbose=false",
    "test:silent": "jest --silent --passWithNoTests",
    "test:header": "jest --testPathPattern=Header.test --passWithNoTests --verbose=false",
    "test:hooks": "jest --testPathPattern=hooks --passWithNoTests"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.288.0",
    "next": "^14.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.15.4",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.200",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "axe-core": "^4.10.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "rimraf": "^5.0.5",
    "size-limit": "^11.2.0",
    "vite": "^7.1.7",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snapurl/admin-panel.git"
  },
  "author": {
    "name": "SnapURL Team",
    "email": "dev@snapurl.dev",
    "url": "https://snapurl.dev"
  },
  "license": "MIT"
}
